            <script id="script_stateCode" type="text/javascript">
            function resultsAFormatResult(results) {       
               //<![CDATA[  
                var markup = "<table class='movie-result'><tr><td>";
                markup += "<div class='movie-synopsis'>" + results.id+" - "+results.title + "</div>";         
                markup += "</td></tr></table>";
                //]]>
                return markup;
            }
            function resultsAFormatSelection(results) { 
                return results.title;
            }
        </script>
        <script id="script_titleCode" type="text/javascript">
            function resultsTFormatResult(results) {       
              //<![CDATA[  
                var markup = "<table class='movie-result'><tr><td>";
                 //]]>
                if(results.title=='Select All'+results.id){
                    //<![CDATA[ 
                    markup += "<div class='movie-synopsis'>" + "All '"+results.id + "' Titles</div>";
                     //]]>
                }else{
                    //<![CDATA[ 
                   markup += "<div class='movie-synopsis'>" + results.title + "</div>";         
                    markup += "</td></tr></table>";
                 //]]>
                }
                return markup;
            }
            function resultsTFormatSelection(results) { 
                if(results.title=='Select All'+results.id){
                    return results.id;
                }else{
                    return results.title;
                }
            }
        </script>
        <script id="script_sicCode" type="text/javascript">    
                $(window).bind("load", function() {
                      $("#sortBy").select2({
                        placeholder: "Select a Sort By" 
                        });
                        $("#sortBy").select2('val','yearlyRevDesc');

                });
                $(window).bind("load", function() {
                      $("#sortByHeader").select2({
                        placeholder: "Select a Sort By" 
                        });
                 $("#sortByHeader").select2('val','yearlyRevDesc');
                });    
        </script>
     </body>
</html>
 <script type="text/javascript"> 
            
    $(document).click(function(event) {
       
        if(event.target != document.getElementById('close_filter_class')){ 
        if(!$(event.target).closest('.overlayDivAlways').length) {
             
            if($('.overlayDivAlways').is(":visible")) {
                 $('.overlayDivAlways').hide(); 
            } 
        } 
        } 
        
    });
    
           
       </script>   
