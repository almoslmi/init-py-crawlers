        $("#updateSearch").click(function(){               
             
            var zip=$('#cityStateCode').val();//alert(zip);
            var range=$('#price_filter').val();//alert(range);
            var state=$('#stateCode').val();//alert(state);
            var title=$('#titleCode').val();//alert(title);
             var technology=$('#technology').val();//alert(technology);  
            var company=$('#company').val();//alert(company);
            var type = $('#titlefilter').val();
            var sic=$('#sicCode').val();//alert(sic);
            var areaCode=$('#areaCode').val();//alert(areaCode);
            var industryCode = $('#industryCode').val();   
            var empRevenue=$("input[name=revenue_ids\\[\\]]:checked").map(function () {return this.value;}).get().join(",");//alert(empRevenue);
            var employee=$("input[name=employee_size\\[\\]]:checked").map(function () {return this.value;}).get().join(",");//alert(employee);
            var employeeLevel =$("input[name=employee_level\\[\\]]:checked").map(function () {return this.value;}).get().join(",");//alert(employeeLevel);
            //var name=$('#name').val();//alert(name);
            //var description=$('#description').val();//alert(description);
            var existingSearchID=$('#existingSearch').val();
            var company_description_string=$('#company_description_string').val();
            var title_keyword=$('#title_keyword').val();
            var currently_hiring_title=$('#currently_hiring_title').val();
            
            var keyword_match_text=$('#keyword_match_text').val();
            
            // var teamShare=$('#teamShare').val();//alert(teamShare);
            var  user_id=$('#user_id').val();          
            var myObj = {};              
            myObj["zip"] =zip;
            myObj["range"] = range;
            myObj["state"]=state;
            myObj["technology"]=technology; 
            myObj["title"]=title;
            myObj["company"]=company;
            myObj["sic"]=sic;
            myObj["areaCode"]=areaCode;
            myObj["industry"]=industryCode;    
            myObj["empRevenue"]=empRevenue;
            myObj["employee"]=employee;
            myObj["employeeLevel"]=employeeLevel;
            myObj["FilterType"]=type;
            myObj["existingSearchID"]=existingSearchID;  
            myObj["company_description_string"]=company_description_string;
            myObj["title_keyword"]=title_keyword;
            myObj["currently_hiring_title"]=currently_hiring_title;
            myObj["keyword_match_text"]=keyword_match_text;
            //myObj["description"]=description;    
            // myObj["teamShare"]=teamShare; 
            myObj["user_id"]=user_id; 
            $.ajax({
                url: 'http://www.lead411.com/search/updateSearch',
                type: 'post',                                        
                data: myObj,
                dataType: "json",
                success: function(data) {
                           
                    if(data.response!="undefined") {
                        //alert(data.response);   
                      /* $('.message').html(data.response);
                        $('.message').show();  */
             if(data.response =="Success: The search updated successfully") {
                         $.jGrowl(data.response, {    
                            sticky: false,
                            theme: 'success'
                             });
                        }else{  //alert(data.response);   
                               $.jGrowl(data.response, { 
                            sticky: false,
                            theme: 'error'
                             });
                        }
                    }
                                  
                }
            });
        
        });
   
