<script id="script_e2">
    $(function() {
        var opts=$("#source").html(), 
        opts2="<option></option>"+opts;    
        $('#e2_2').html(opts2); 
    });
               
               
           
$(document).ready(function() {
    function formatNoMatchesText(term) { 
       if($('#e2_2').val()!=null && $('#e2_2').val().indexOf('-1', 0)>-1){ 
                 return 'No more choice';
             }else{  
                return 'No matched found';
             } 
        }
    
    $("#e2_2").select2({
         placeholder: "Trigger On", 
         matcher: function(term, text) { 
             if($('#e2_2').val()!=null && $('#e2_2').val().indexOf('-1', 0)>-1){ 
                 return '';
             }else{  
                return text.toUpperCase().indexOf(term.toUpperCase())==0; 
             }
         } ,
        formatNoMatches : formatNoMatchesText 
          
    });
    
    $("#e2_2").click(function() {
        if($('#e2_2').val()!=null && $('#e2_2').val().indexOf('-1', 0)>-1){
             formatNoMatches:formatNoMatchesText; 
         $("#e2_2").select2("data", {id: "-1", text: "All Events"}); 
       } 
    });
});
    $(document).ready(function() {
       // $(".menu_head").click();
        $(".menu_head").css("backgroundImage","url(http://static.lead411.com/images/down.png)");
//        var dataTables_info=  $(".dataTables_info").detach(); //Performs the cut operation
//        $(".totalRecords").append(dataTables_info);
//    
        var temp=  $(".dataTables_filter").detach(); //Performs the cut operation        
        $(".filter_div").append(temp);
        //temp.insertBefore(".result_found_div");  //Does the paste
        
         var inside=false; 
          var insideOpen=false; 
         
         function download_popup_content() {
              if (inside==true) {  
                 $(".download_popup_content").animate({ opacity: 'hide' }, "slow");
                  inside==false;
                }
          } 
              
           $('.download_popup_content, .download_btn').mouseover(function() {
                  inside = false;
           });
            $('.download_popup_content, .download_btn').mouseout(function() {
                  inside = true;
                  setTimeout(download_popup_content, 750); 
           });
           
           function search_popup_content() {
            if (insideOpen==true) {  
               $(".search_popup_content").animate({ opacity: 'hide' }, "slow");
                inside==false;
              }
          } 
              
           $('.search_popup_content, .saved_search').mouseover(function() {
                  insideOpen = false;
           });
            $('.search_popup_content, .saved_search').mouseout(function() {
                  insideOpen = true;
                  setTimeout(search_popup_content, 750); 
           });
           
        
}); 
    
</script>


