            myObj["description"]=description;    
            myObj["teamShare"]=teamShare; 
            myObj["user_id"]=user_id; 
            myObj["company_description_string"]=company_description_string;
            myObj["title_keyword"]=title_keyword;
            
            myObj["currently_hiring_title"]=currently_hiring_title;
            myObj["keyword_match_text"] = keyword_match_text;
//            myObj[''] = 
            
            $.ajax({
                url: 'http://www.lead411.com/search/saveSearech',
                type: 'post',                                        
                data: myObj,
                dataType: "json",
                success: function(data) {                   
                  if(data.response!="undefined") {                      
                      /* $('.message').html(data.response);
                        $('.message').show();  
                        $(".track_popup_content").hide(); 
                        $(".download_popup_content").hide();*/
                        if(data.response == 'Success: The search saved successfully'){
                            //alert(data.response); 
                           //alert(data.id);
                           if ( $('#my_search_list li:contains("No Searches")').length ) {
                              $('#my_search_list li:contains("No Searches")').remove();
                               $("<li id='edit_searches_id' style='border-top: 1px solid #005B7B;margin-top: 5px;padding-top: 2px;'><a href='http://www.lead411.com/accountsettings/editsearches'>Edit Searches</a></li>").appendTo($('#my_search_list')); 
                                var ul = document.getElementById("my_search_list");
                                 var li = document.createElement("li");
                            //li.appendChild(document.createTextNode('<a href="http://www.lead411.com/search/recall_search/'+data.id+'">'+name+'</a>'));
                            li.innerHTML='<a href="http://www.lead411.com/search/recall_search/'+data.id+'">'+name+'</a>';
                            //ul.insertBefore(li, document.getElementById("edit_searches_id"));                           
                                    if($('#my_territory_id').length){
                                        $('#edit_searches_id').css('border-top','0');
                                        ul.insertBefore(li, document.getElementById("my_territory_id"));
                                    }else{
                                        ul.insertBefore(li, document.getElementById("edit_searches_id"));  
                                    }
                            }else{
                            var ul = document.getElementById("my_search_list");
                            var li = document.createElement("li");
                            //li.appendChild(document.createTextNode('<a href="http://www.lead411.com/search/recall_search/'+data.id+'">'+name+'</a>'));
                            li.innerHTML='<a href="http://www.lead411.com/search/recall_search/'+data.id+'">'+name+'</a>';
                            //ul.insertBefore(li, document.getElementById("edit_searches_id"));
                             //ul.insertBefore(li, document.getElementById("my_territory_id"));
                             if($('#my_territory_id').length){
                                        ul.insertBefore(li, document.getElementById("my_territory_id"));
                                    }else{
                                        ul.insertBefore(li, document.getElementById("edit_searches_id"));  
                                    }
                            }
                             $.jGrowl(data.response, {    
                            sticky: false,
                            theme: 'success'
                             });
                        }else{  //alert(data.response);   
                               $.jGrowl(data.response, { 
                            sticky: false,
                            theme: 'error'
                             });
                        }
                    }
                }
            });
        });
