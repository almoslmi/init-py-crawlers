        $('.all_new_btn').click(function(){
            
            formFunction();
            
            //var format=$('.radio_btn2').val();
            var format = $('input[name=download_all]:checked').val();
            
            var zip=$('#cityStateCode').val();//alert(zip);
            var range=$('#price_filter').val();//alert(range);
            var state=$('#stateCode').val();//alert(state); 
            var title=$('#titleCode').val();//alert(title);
            
            //var company=$('#company').val();//alert(company);
            var type = $('#titlefilter').val();// alert(type);
            var sic=$('#sicCode').val();//alert(sic);
            var technology=$('#technology').val();//alert(technology);  
            var areaCode=$('#areaCode').val();//alert(areaCode);
            var industryCode = $('#industryCode').val();
            var user_search_string=$('#company').val();

            var FilterRevenue=$("input[name=revenue_ids\\[\\]]:checked").map(function () {return this.value;}).get().join(",");//alert(empRevenue);
            var FilterEmployee=$("input[name=employee_size\\[\\]]:checked").map(function () {return this.value;}).get().join(",");//alert(employee);
            var FilterLevel =$("input[name=employee_level\\[\\]]:checked").map(function () {return this.value;}).get().join(",");//alert(employeeLevel);
            
            var Press_event_data = $('#Press_event_data').val();
            var company_description_string=$('#company_description_string').val();
            var title_keyword=$('#title_keyword').val();
            var currently_hiring_title=$('#currently_hiring_title').val();
            var keyword_match_text=$('#keyword_match_text').val();
            
            //var FilterType = $('input[name=companyResults]:checked').val();
            
            
            var myObj = {};              
            myObj["zip"] =zip;
            myObj["range"] = range;
            myObj["state"]=state;
            myObj["title"]=title;
            myObj["user_search_string"]=user_search_string;
            myObj["sic"]=sic;
            myObj["technology"]=technology;
            myObj["areaCode"]=areaCode;
            myObj["industryCode"]=industryCode;
            myObj["FilterRevenue"]=FilterRevenue;
            myObj["FilterEmployee"]=FilterEmployee;
            myObj["FilterLevel"]=FilterLevel;
            myObj["FilterType"]=type;
            myObj["Press_event_data"]=Press_event_data;
            myObj["currently_hiring_title"]=currently_hiring_title;
            myObj["company_description_string"]=company_description_string;
            myObj["title_keyword"]=title_keyword;
            myObj["keyword_match_text"]=keyword_match_text;
            
            var enterprise_package_id = '[14,15]';
            var enterprise_ids = $.parseJSON(enterprise_package_id);
            
            var package_id = '10';
            
            var check = $.inArray(parseInt(package_id), enterprise_ids);
            
            //if(package_id == $.parseJSON(enterprise_package_id)){ 
          
            
         //if(package_id == $.parseJSON(enterprise_package_id)){
         if(check >= 0){ 
            //if(package_id = $.parseJSON(package_id)){ 
            
                var text = $(".dataTables_info").text();
                var total = text.split(" (");
                var total_contacts = parseFloat(total[0].replace(',','').replace(' ',''));
                //var limit = 100;
                
                if(total_contacts == 0){
                    var message = 'No search result found. Please try some another search criteria.';
                            show_dialog(message);
                            return false;
                }
                
                
                var limit = 5000;
                
                if(total_contacts > limit ){ 
                    $('.lightface').hide();    
                    $('#overlay').remove();
                    $.post('http://www.lead411.com/search/saveToEnterpriseDownloads', {
                        'data': myObj,
                        'format': format 
                    },
                    function(data) { 
                        
                        if (data == 'show_dialog') {
                            var message = 'The link of the file containing contacts will be emailed to you shortly.';
                            show_dialog(message);
                        } else if (data == 'process') {
                            var message = 'File requested is already being created, please wait...';
                            show_dialog(message);
                        } else {
                            var message = 'There was some error in downloading the file. Please try again.';
                            show_dialog(message);
                        }
                    });
                    
                    return false;
                } 
            //} 
           } 
            
            //alert('3333'); return false;
            
            $.post('http://www.lead411.com/search/saveEnterpriseDownloads', {
                'data': myObj,
                'format': format 
            },
            function(data) { 
                
                //alert(data); return false;
                
                if (data == 'continue') {  
                    
                    submitDownload(2); 
                    
                    $('.lightface').hide();    
                    $('#overlay').remove();
                } else if (data == 'show_dialog') {
                    $('.lightface').hide();    
                    $('#overlay').remove();
                    var message = 'The link of the file containing contacts will be emailed to you shortly.';
                    show_dialog(message);
                } else if (data == 'process') {
                    $('.lightface').hide();    
                    $('#overlay').remove();
                    var message = 'File requested is already being created, please wait...';
                    show_dialog(message);
                }  
            });
            return false;
        });
        
        
        
    
    });  
    
    
    function submitDownload(sel) {
    if ($(".case").length > 0) {

        $('#select_action_download').val(sel);
        var compid = '';
        var empid = '';
        $(".case").each(function() {
            if ($(this).is(":checked") == true) {
                if ($(this).attr("id").indexOf("comp_") >= 0) {
                    compid = compid + $(this).attr("id").split("_")[2] + ",";
                } else if ($(this).attr("id").indexOf("emp_") >= 0) {
                    empid = empid + $(this).attr("id").split("_")[2] + ",";
                    compid = compid + $(this).attr("id").split("_")[3] + ",";
                }
            }
        });


        var query = $("<input></input>");
        $(query).val($("#searched_str").val());
        $(query).attr("name", "query");
        $("#downloadFrm").append($(query));
        $("#company_id").val(compid);
        $("#employee_id").val(empid);
        $(query).hide();

        if (sel == 1) {
            var companies = $("#company_id").val();
            var employees = $("#employee_id").val();
            var format = $('input[name=download_all]:checked').val();



            if (companies == '') {
                var message = "You haven't selected any company or person.";
                //show_dialog(message); 
                return false;
            }
            $.post('http://www.lead411.com/search/saveCheckedEnterpriseDownloads', {
                'company_id': companies,
                'employee_id': employees,
                'format': format
            },
            function(data) {
                
                if (data == 'continue') {
                    $('#downloadFrm').submit();
                } else if (data == 'show_dialog') {
                    var message = 'The link of the file containing contacts will be emailed to you shortly.';
                    show_dialog(message);
                } else if (data == 'process') {
                    var message = 'File requested is already being created, please wait...';
                    show_dialog(message);
                } else {
                    var message = 'There was some problem in downloading. Please try again.';
                    show_dialog(message);
                }

            });
            return false;
        }
        $('#downloadFrm').submit();

        return false;
    }
}
function formFunction() {
    var overlay = $('<div></div>').prependTo('body').attr('id', 'overlay');
    var box = new LightFace({
        title: 'File Download',
        content: '<img style="margin: 0 0 0 38px;" src="http://www.lead411.com//assets/images/ajax-loader-download.gif">',
        draggable: true
    });
    box.open();
}
  //insert tracking of customer
    function insertTracking(emp_id,type){
    
          if(type=='salesforce'){
              _kmq.push(['identify', 'm.kumar@innovaccer.com']);
              _kmq.push(['record', 'Salesforce Push']);
              _kmq.push(['set', {"Ip":"103.28.255.207","City":"Delhi","Region":"07","Country":"IN","Latitude":28.66670036315918,"Longitude":77.21669769287109}]);
          }
          $.ajax({
            url: 'http://www.lead411.com/profile_page/insertTrackingData',
            data:{employee_id:emp_id,click_type:type},
            type: 'post',
            success: function(datastr) { 
               if(type=="vcard"){
                   window.location='http://www.lead411.com/vcard/index/'+emp_id;
               }
            }
        });
    }
    
    
