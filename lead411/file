			function fnFormatDetails ( nTr )
			{
				var aData = oTable.fnGetData( nTr );
                                var sOut = '<div class="cdetails innerDetails" style="display: none;">\n\
                                <table bgcolor=#d9d9d9 class="inner_table" cellpadding="5" cellspacing="0" border="0" style="margin-left: 3%; border: 0px dotted;padding-left: 0px;width: 93%; height:auto;">';
				  
                                  
                                                                       sOut +='<tr style="background: #FFFFFF; "><td valign="top" style="text-align:justify !important;"> <div class="left_inner_boxsearch">' +
                                    '<div class="search_info">Company Info</div><br><div style="margin:5px -5px 2px;"><hr></div><div class="search_info"><b></b>' + aData['clink'] + '</div></br>' + '<div class="search_info"><b></b>  ' + aData['address']+ '</div></br>';
                                 
                                    if(aData['address2'].trim()!=''){
                                        sOut +='<div class="search_info"><b></b>  ' + aData['address2']+ '</div></br>';
                                    }
                                    sOut +='<div class="search_info"> ' + aData['city'] + ', '+aData['state']+ ' '+aData['zip'] + '</div></br>';
                                    if(aData['phone'].trim()!=''){
                                        sOut +='<div class="search_info">P: ' + aData['phone']  + '</div></br>';
                                    }
                                    if(aData['fax'].trim()!=''){
                                        sOut +='<div class="search_info">F: ' + aData['fax'] + '</div></br>';
                                    }
                                    sOut +='</div></td>';
                                    
                                                                        
                                    sOut += ' <td valign="top" colspan="2" style="text-align:justify !important;"><div class="left_inner_boxsearch">\n\
                                    <div class="search_info">Company Details</div><br><div style="margin:5px -5px 2px;"><hr></div>\n\
                                    <div class="search_info1">Employees: <a  onclick="signup1();" href="#login-box" style="color:#0092FF;">Free Trial to view</a></div>\n\
                                    <div class="search_info1">Revenue: <a onclick="signup1();" href="#login-box" style="color:#0092FF;">Free Trial to view</a></div>\n\
                                    <div class="search_info1">Industry: <a onclick="signup1();" href="#login-box" style="color:#0092FF;">Free Trial to view</a></div>\n\
                                    <div class="search_info1"><div style="text-align:left;width:82%;"><div style="text-align:left;">SIC Code:';
                                    
                                    
                                     
                                        
                                    if(aData['sic_code']!=null && aData['sic_code']!='' && aData['sic_code'].trim()!=''){
                                        sOut +=' ' + aData['sic_code']+'-'+aData['description']+ '</div></div></br>';
                                    }else{
                                        sOut +=' No SIC Codes Found</div></div></br>';
                                    }  
                                    
                                    sOut +='</div></div></td>';
                                   
                                   sOut += ' <td valign="top" bgcolor=#FFFFFF colspan="2" style="text-align:justify !important;">\n\
                                    <div class="left_inner_boxsearch">\n\
                                    <div class="search_info">Company Contacts</div><br><div style="margin:5px -5px 2px;"><hr></div>\n\
                                    <div class="search_info2">Total: ' + aData['total_contacts'] + '</div>\n\
                                    <div class="search_info2">Executive Level: ' + aData['executive_level'] + '</div>\n\
                                    <div class="search_info2">VP Level: ' + aData['vp_level'] + '</div>\n\
                                    <div class="search_info2">Director Level: ' + aData['director_level'] + '</div>\n\
                                    <div class="search_info2">Manager Level: ' + aData['manager_level'] + '</div>\n\
                                    <div class="search_info2">Others: ' + aData['other'] + '</div>\n\
                                    </div></td>'; 
                                        
                                    sOut += '</tr></table></div>';			
				return sOut;
			}
