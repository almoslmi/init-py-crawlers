 <script type="text/javascript"> 
            
    $(document).click(function(event) {
       
        if(event.target != document.getElementById('close_filter_class')){ 
        if(!$(event.target).closest('.overlayDivAlways').length) {
             
            if($('.overlayDivAlways').is(":visible")) {
                 $('.overlayDivAlways').hide(); 
            } 
        } 
        } 
        
    });
    
           
       </script>   