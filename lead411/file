                                                chk_employeeVals = [];
                                            }  
                                            $('.chk_employee:checked').each(function() { 
                                                chk_employeeVals[index]=  $(this).val()  ;
                                                index++;
                                            });  

                                           if(chk_employeeVals.length==1){
                                             var i=parseInt(chk_employeeVals[0])+26; 
                                             $("#employee_size_"+i).attr('checked', true);
                                            }
					fnFilterColumn (19);
				});
				$(".chk_employee_level").click(function() {
					//alert('inside chk_employee_level');
					fnFilterColumn (21);
				});
				
				//$("#titlefilter").keyup( function() { fnFilterColumn( 8 ); } );
				$('#titlefilter').keyup(function() {

					   $('#example').dataTable().fnFilter( 
							$(this).val(),
							8, 
							true,
						   true
							);
						
						//	$('#example').dataTable().fnMultiFilter( { "clink": $(this).val(), "plink": $(this).val() } );
							
				 });
				/* Add event listener for opening and closing details
				 * Note that the indicator for showing which row is open is not controlled by DataTables,
				 * rather it is done here
				 */
                               
                                  
                                
                                $('#example').find('.checkboxClass').live('mouseover', function () { 
                                   
                                    return false;
                                });
                                $('#example').find('.checkboxClass').live('mouseenter', function () { 
                                   
                                    return false;
                                });
                                $('#example').find('.checkboxClass').live('mouseout', function () { 
                                    
                                    return false;
                                });
                                $('#example').find('.checkboxClass').live('click', function (event) { 
					var nTr = this.parentNode;                                         
                                        // var index = $(this).index(); 
                                          var $target = $(event.target);
                                           
                                          if ( $target.is("a") &&  $target.text()=='Sign Up') { 
                                                return false; 
                                             }
                                             
                                             if ( $target.is("div") ) { 
                                              nTr = nTr.parentNode; 
                                             }

					if($(nTr).find('td img.empl').attr('src')==sImageUrl+"details_close.png"){
							$(nTr).find('td img.empl').attr('src', sImageUrl+"details_open.png");
						}
					
					iE = $.inArray( nTr, anOpenE );
					if (iE === -1 ) 
					{ 
						/* Open this row */
						$(nTr).find('td img.comp').attr( 'src', sImageUrl+"details_close.png" );
                                                var nDetailsRow = oTable.fnOpen( nTr, fnFormatDetails(nTr), 'details' );
						$('div.innerDetails', nDetailsRow).slideDown('slow');
						$(".popbox-link").click(function(){
							$(this).parent().closest('table').parent().find(".popbox4").show();
							//$(".price_data").load("yahoo_data.php");
		  				});
						$(".flag_popbox_link").click(function(){
							$(this).parent().closest('table').parent().parent().find(".flag_popbox").show();
							
		  				});
						$(document).ready(function(){
						$(".flag_popbox .close").click(function(){
						$(this).parents(".flag_popbox").animate({ opacity: 'hide' }, "slow");
						});

						});
						
						$(document).ready(function(){
						$(".popbox4 .img_close").click(function(){
						$(this).parents(".popbox4").animate({ opacity: 'hide' }, "slow");
						});

						});
						
					
						anOpenE.push( nTr );
						anOpen.splice( i, 1 );
						
						
					}
					else
					{
						/* This row is already open - close it */
						$(nTr).find('td img.comp').attr( 'src', sImageUrl+"details_open.png" );
						$('div.innerDetails', $(nTr).next()[0]).slideUp('fast', function () {
						 oTable.fnClose( nTr );
						 anOpenE.splice( iE, 1 );
					   } );

					}
                                });
                                
                                var flag=false;
                                 $('#example').find('.comp').live('mouseover', function () { 
					var nTr1 = this.parentNode;
					var nTr = nTr1.parentNode; 
                                      //  $(nTr).find('td img.comp').attr( 'src', sImageUrl+"details_open.png" ); 
                                      //  alert($(nTr).find('td img.comp').attr('src'));
                                        if(flag==false){
                                            if($(nTr).find('td img.comp').attr('src')==sImageUrl+"details_close.png"){
							$(nTr).find('td img.comp').attr('src', sImageUrl+"details_close_orange_new.png");
					     }else{
                                                 $(nTr).find('td img.comp').attr('src', sImageUrl+"details_open_orange_new.png"); 
                                             } 
                                            flag=true;
                                        } 
                                }); 
                                
                                $('#example').find('.comp').live('mouseout', function () { 
					var nTr1 = this.parentNode;
					var nTr = nTr1.parentNode; 
                                       // alert($(nTr).find('td img.comp').attr('src'));
                                        
                                         if(flag==true){
                                             flag=false; 
                                              
                                             
                                             if($(nTr).find('td img.comp').attr('src')==sImageUrl+"details_close.png"){
						 $(nTr).find('td img.comp').attr('src', sImageUrl+"details_close_orange_new.png");
					     }else if($(nTr).find('td img.comp').attr('src')==sImageUrl+"details_close_orange_new.png"){
						 $(nTr).find('td img.comp').attr('src', sImageUrl+"details_close.png");
					     }
                                             else{
                                                 $(nTr).find('td img.comp').attr('src', sImageUrl+"details_open.png"); 
                                             } 
                                         } 
                                }); 
//                                 $('#example').find('.comp').live('mouseenter', function () {
//                                      
//					var nTr1 = this.parentNode;
//					var nTr = nTr1.parentNode; 
//					 $(nTr).find('td img.comp').attr('src', sImageUrl+"details_open_orange.png"); 
//                                        
//                                }); 
                                
                                
				$('#example').find('.comp').live('click', function () {
					var nTr1 = this.parentNode;
					var nTr = nTr1.parentNode;
                                           
					if($(nTr).find('td img.empl').attr('src')==sImageUrl+"details_close.png"){
							$(nTr).find('td img.empl').attr('src', sImageUrl+"details_open.png");
						}
					
					iE = $.inArray( nTr, anOpenE );
					if (iE === -1 ) 
					{ 
						/* Open this row */
						//$(this).attr( 'src', sImageUrl+"details_close.png" );
                                                if($(this).attr('src')==sImageUrl+"details_open_orange_new.png"){
							$(this).attr('src', sImageUrl+"details_close_orange_new.png");
					     }else{
                                                 $(this).attr('src', sImageUrl+"details_close.png"); 
                                             } 
                                             
                                                var nDetailsRow = oTable.fnOpen( nTr, fnFormatDetails(nTr), 'details' );
						$('div.innerDetails', nDetailsRow).slideDown('slow');
						$(".popbox-link").click(function(){
							$(this).parent().closest('table').parent().find(".popbox4").show();
							//$(".price_data").load("yahoo_data.php");
		  				});
						$(".flag_popbox_link").click(function(){
							$(this).parent().closest('table').parent().parent().find(".flag_popbox").show();
							
		  				});
						$(document).ready(function(){
						$(".flag_popbox .close").click(function(){
						$(this).parents(".flag_popbox").animate({ opacity: 'hide' }, "slow");
						});

						});
						
						$(document).ready(function(){
						$(".popbox4 .img_close").click(function(){
						$(this).parents(".popbox4").animate({ opacity: 'hide' }, "slow");
						});

						});
						
					
						anOpenE.push( nTr );
						anOpen.splice( i, 1 );
						
						
					}
					else
					{
						/* This row is already open - close it */
                                                if($(this).attr('src')==sImageUrl+"details_close_orange_new.png"){
							$(this).attr('src', sImageUrl+"details_open_orange_new.png");
                                                    }else{
                                                        $(this).attr('src', sImageUrl+"details_open.png"); 
                                                    } 
                                             
						 //$(this).attr( 'src', sImageUrl+"details_open.png" );
						$('div.innerDetails', $(nTr).next()[0]).slideUp('fast', function () {
						 oTable.fnClose( nTr );
						 anOpenE.splice( iE, 1 );
					   } );

					}
				} );
                                
                                
				$('#example').find('.empl').live('click', function () {
					var nTr1 = this.parentNode;
					var nTr = nTr1.parentNode;
					if($(nTr).find('td img.comp').attr('src')==sImageUrl+"details_close.png"){
							$(nTr).find('td img.comp').attr('src', sImageUrl+"details_open.png");
						}
					i = $.inArray( nTr, anOpen );
					if ( i === -1 ) 
					{
						/* Open this row */
						$(this).attr( 'src', sImageUrl+"details_close.png" );
						 var nDetailsRow = oTable.fnOpen( nTr, fnEmployeeDetails( nTr), 'details' );
						$('div.innerDetails', nDetailsRow).slideDown('fast');
						anOpen.push( nTr );
						anOpenE.splice( iE, 1 );
						
						$(".flag_contact_link").click(function(){
						$(this).parent().closest('table').parent().parent().find(".flag_contact").show();
						});                $DataTablesElement['hlink'] = $row['url'];

						
						$(document).ready(function(){
						$(".flag_contact .close").click(function(){
						$(this).parents(".flag_contact").animate({ opacity: 'hide' }, "slow");
						});

						});
						

					}
					
					else
					{
						/* This row is already open - close it */
						
						 $(this).attr( 'src', sImageUrl+"details_open.png" );
						$('div.innerDetails', $(nTr).next()[0]).slideUp('fast', function () {
						 oTable.fnClose( nTr );
						 anOpen.splice( i, 1 );
					   } );

					}
				} );


				// add multiple select / deselect functionality
				$("#selectall").click(function () {
					  $('.case').attr('checked', this.checked);
				});
				// if all checkbox are selected, check the selectall checkbox
				// and viceversa
				$(".case").click(function(){
			 
					if($(".case").length == $(".case:checked").length) {
						$("#selectall").attr("checked", "checked");
					} else {
						$("#selectall").removeAttr("checked");
					}
			 
				});

			} );
